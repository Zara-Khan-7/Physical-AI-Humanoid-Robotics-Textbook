# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Site URL (for Vercel deployment)
SITE_URL=https://your-project.vercel.app

# Base URL (use '/' for Vercel, '/Physical-AI-Humanoid-Robotics-Textbook/' for GitHub Pages)
BASE_URL=/

# Backend API URL (your deployed backend URL)
API_URL=https://your-backend.railway.app/api/v1

# GitHub Pages specific (only needed if deploying to GitHub Pages)
ORG_NAME=your-github-username
PROJECT_NAME=Physical-AI-Humanoid-Robotics-Textbook
